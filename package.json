{
  "name": "@beequeue/anilist-changelog",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@6.32.14",
  "engines": {
    "node": ">=16"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,json5,yaml,yml,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "start": "node dist/main.js",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@actions/core": "1.8.2",
    "@beequeue/eslint-plugin": "0.5.0",
    "@graphql-inspector/core": "3.1.2",
    "@tsconfig/node16": "1.0.2",
    "@types/node": "16.11.36",
    "dotenv": "16.0.1",
    "eslint": "8.15.0",
    "graphql": "16.5.0",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "simple-git-hooks": "2.7.0",
    "ts-dedent": "2.2.0",
    "tsup": "5.12.8",
    "typescript": "4.7.0-dev.20220506",
    "undici": "5.2.0",
    "zod": "3.16.0"
  }
}
