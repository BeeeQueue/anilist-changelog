{
  "name": "@beequeue/anilist-changelog",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@7.20.0",
  "engines": {
    "node": ">=16.15"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,json5,yaml,yml,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "start": "node dist/main.js",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@actions/core": "1.10.0",
    "@beequeue/eslint-plugin": "1.1.1",
    "@graphql-inspector/core": "3.4.0",
    "@tsconfig/node16": "1.0.3",
    "@types/node": "16.18.10",
    "dotenv": "16.0.3",
    "eslint": "8.30.0",
    "graphql": "16.6.0",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "simple-git-hooks": "2.8.1",
    "ts-dedent": "2.2.0",
    "tsup": "6.5.0",
    "typescript": "4.9.4",
    "undici": "5.14.0",
    "zod": "3.20.2"
  }
}
